# File created by KDE Gtk Config
# Configs for GTK2 programs 

include "/usr/share/themes/elementary/gtk-2.0/gtkrc"
style "user-font" 
{
	font_name="Open Sans Regular"
}
widget_class "*" style "user-font"
gtk-font-name="Open Sans Regular 10"
gtk-theme-name="elementary"
gtk-icon-theme-name="elementary"
gtk-fallback-icon-theme="elementary"
gtk-cursor-theme-name="elementary"
gtk-toolbar-style=GTK_TOOLBAR_BOTH_HORIZ
gtk-menu-images=1
gtk-button-images=1
gtk-primary-button-warps-slider=0
gtk-modules=appmenu-gtk-module
gtk-sound-theme-name = elementary
